package assignment3;

public class Stack<T> {
	
	
	public Stack() {
		
	}
	
	private StackNode<T> top;

	public T peek() {
		return top.getData();
	}
	
	public T pop() {
		T item = top.getData();
		top = top.next;
		return item;
	}
	
	public void push(StackNode<T> node) {
		node.next = top;
		top = node;
	}
	
	public boolean isEmpty() {
		if (top.getData() == null) {
			return true;
		}
		else {
			return false;
		}
	}
}


